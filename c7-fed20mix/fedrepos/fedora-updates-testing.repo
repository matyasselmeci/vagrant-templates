[fedora-updates-testing]
name=Fedora 20 - $basearch - Test Updates
failovermethod=priority
#baseurl=http://download.fedoraproject.org/pub/fedora/linux/updates/testing/20/$basearch/
metalink=https://mirrors.fedoraproject.org/metalink?repo=updates-testing-f20&arch=$basearch
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-fedora-20-$basearch
skip_if_unavailable=True

